define(['jquery','ko','uiComponent'],function($,ko,Component){'use strict';return Component.extend({initialize:function(){this._super();this.qty=ko.observable(this.defaultQty);},decreaseQty:function($data){var qty=parseInt(document.getElementById($data).value);var sku=$(document.getElementById($data)).attr('data-cart-item-id');try{if(window.shopti_data_layer.product_skus){var itemIndex=window.shopti_data_layer.product_skus.indexOf(sku);if(itemIndex){var event_properties={name:window.shopti_data_layer.product_names[itemIndex],sku:sku,price:window.shopti_data_layer.product_prices[itemIndex],qty:window.shopti_data_layer.product_qty[itemIndex]-1}
var event=new CustomEvent('QtyMinus',{detail:{event_properties:event_properties}});document.dispatchEvent(event);}}}catch(error){console.log('Could not fire Tracker Event',error)}
var newQty=qty-1;if(newQty<1){newQty=1;}
document.getElementById($data).value=newQty;if(document.body.classList.contains('checkout-cart-index')){var update_cart=document.getElementById("update_cart_button");update_cart.click();}},increaseQty:function($data){var qty=parseInt(document.getElementById($data).value);var newQty=qty+1;document.getElementById($data).value=newQty;if(document.body.classList.contains('checkout-cart-index')){var update_cart=document.getElementById("update_cart_button");update_cart.click();}}});});